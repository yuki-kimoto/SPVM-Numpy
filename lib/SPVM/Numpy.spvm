class Numpy {
  use Numpy::Ndarray;
  
  static method new : Numpy () {
    my $self = new Numpy;
    
    return $self;
  }
  
  static method zeros : Numpy::Ndarray ($shape : int[], $dtype = -1 : byte) {
    
    my $ndarray = Numpy::Ndarray->new_zeros($shape, $dtype);
    
    return $ndarray ;
  }
  
}
